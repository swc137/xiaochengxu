page {
  background-color: #F8F8F8;
  height: 100%;
  font-size: 28rpx;
  line-height: 1.6;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.overflow{
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.more-overflow{
  width: 100%;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.hidden{
  display:none;
}
.show{
  display: block;
}
/*标题*/
.p-title{
  padding: 0 30rpx;
  background-color: #fff;
  margin-top: 20rpx;
  line-height: 80rpx;
  position: relative;
  border-bottom: 1px solid #eee;
}
.p-title image{
  width: 26rpx;
  height: 26rpx;
  position:absolute;
  top:0;
  bottom:0;
  margin: auto;
}
.p-title .p-text{
  color: #666;
  margin-left: 50rpx;
}
/*在线客服*/
.page-body-line{
  position:fixed;
  bottom:60rpx;
  right:20rpx;
  width:78rpx;
  height:78rpx;
  background-color: #2072cf;
  border-radius: 50%;
}
.page-body-line contact-button{
  position: absolute;
  top: 14rpx;
  left: 12rpx;
}
/*字体颜色*/
.color-6{
  color:#666;
}
.color-3{
  color:#333;
}
.color-9{
  color:#999;
}
.color-o{
  color: #ee8722;
  font-size: 30rpx;
}
/*边框线*/
.border-b{
  border-bottom: 2rpx solid #ddd;
}
.more{
  float: right;
  color: #5299c7;
}
.right-arrow {
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 30rpx;
  border: none;
  border-left: 0;
  border-bottom: 0;
  border-top: 1px solid #969696;
  border-right: 1px solid #969696;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.mar-t{
  margin-top: 20rpx;
}
/*表单信息*/
.f-right{
  width: 70%;
  float: left;
}
form input{
  background-color: #fff;
  padding: 0;
  float: right;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: right;
}
form picker{
  text-align: right;
  width: 95%;
}
.f-line{
  position: relative;
  background-color: #fff;
  overflow: hidden;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 2rpx solid #eee;
  padding-right:30rpx;
}
.f-title{
  width: 25%;
  float: left;
  padding-left: 5%;
}
form button{
  margin: 40rpx 4%;
  width: 92%;
  background-color: #ee8722;
  color: #fff;
}