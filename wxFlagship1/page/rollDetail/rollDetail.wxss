.roll-list{
  padding: 30rpx;
}
.roll-item{
  background: #fff;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.item-left{
  color: #ff0000;
  text-align: center;
  float: left;
  width: 30%;
  border-right: 2rpx dashed #ddd;
  padding: 20rpx 0;
}
.roll-num{
  font-size: 40rpx;
}
.item-right{
  float: left;
  width: 65%;
  padding: 20rpx 2%;
}
.roll-time{
  color: #999;
  font-size: 24rpx;
  overflow: hidden;
  margin-top: 28rpx;
}
.roll-date{
  float: right;
}
.has-roll{
  background-color: #f63d50;
  color: #fff;
  margin-top: 20rpx;
}