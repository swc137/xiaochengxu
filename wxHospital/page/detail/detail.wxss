.banners{
  width: 100%;
}
.menu{
  padding: 16rpx 30rpx;
  overflow: hidden;
  background-color: #fff;
}
.icon-menu{
  width: 52rpx;
  height: 68rpx;
  float: left;
  margin-right: 30rpx;
}
.menu text{
  display: inline-block;
  height: 56rpx;
  line-height: 56rpx;
  width: 184rpx;
  text-align: center;
  padding-right: 10rpx;
  font-size: 32rpx;
}
.menu-list{
  border-bottom: 2rpx solid #ddd;
  float: left;
}
.menu-list .active{
  background-image: url('https://xiaochengxu.zhuniu.com/assets/pages/img/bg-menu.png');
  background-repeat: no-repeat;
  background-position: center center;
  background-size: 100% 100%;
  color: #fff;
}
.doctor-item{
  padding: 30rpx 30rpx 70rpx;
  overflow: hidden;
  background-color: #fff;
  position: relative;
}
.doctor-info{
  width: 53%;
  position: absolute;
  left: 318rpx;
}
.doctor-img{
  float: left;
  width: 318rpx;
}
.f-28{
  font-size: 28rpx;
}
.f-b{
  font-weight: 800;
}
.color-b{
  color:#15549b;
}
.f-20{
  font-size: 20rpx;
} 
.color-0{
  color: #000;
  font-weight: 400;
  margin-left: 20rpx;
}
.border-b{
  border-bottom: 2rpx solid #15549b;
  margin-bottom: 50rpx;
}
.doctor-detail,.order{
  display: inline-block;
  padding: 8rpx 20rpx;
  background-color: #0b73d7;
  border-radius: 6rpx;
  color: #fff;
  margin-top: 16rpx;
}
.order{
  background-color: #81a9d9;
  float: right;
}
.banner-text{
  background-color: #fff;
  margin: 20rpx 0;
  height: 96rpx;
  text-align: center;
}
.banner-text image{
  width: 50%;
  height:96rpx;
}
.ad-img{
  width: 100%;
  overflow: hidden;
  margin: 50rpx 0;
}
.ad-img image{
  float: left;
  height: 212rpx;
}
.ad-img .img1{
  width: 37%;
  margin: 0 2% 0 4%;
}
.ad-img .img2{
  width: 26%;
  margin-right: 2%;
}
.ad-img .img3{
  width: 25%;
}
.bottom-content{
  text-align: center;
  background-color: #fff;
  padding: 80rpx 30rpx;
  font-size: 22rpx;
  padding-bottom: 200rpx;
}
.tips{
  width: 100%;
  height: 20rpx;
}
.b-menu{
  height: 120rpx;
  background-color: #f0f0f0;
  overflow: hidden;
  position: fixed;
  width: 100%;
  bottom: 0;
}
.menu-i,.menu-q,.menu-y{
  float: left;
  width: 33%;
  line-height: 120rpx;
  position: relative;
  overflow: hidden;
}
.menu-i image,.menu-q image,.menu-y image{
  width: 63rpx;
  margin-top: 30rpx;
  margin-left: 36rpx;
  float: left;
}
.menu-i image{
  margin-left: 70rpx;
}
.menu-i,.menu-q{
  border-right: 2rpx solid #fff;
}
.b-menu text{
  float: left;
}

.left-weapp{
  position:absolute;
  bottom:0;
  left:0;
  opacity: 0;
}
.right-weapp{
  position:absolute;
  bottom:0;
  right:0;
  opacity: 0;
}