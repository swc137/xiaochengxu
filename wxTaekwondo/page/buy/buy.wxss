.bg-w{
  width: 100%;
  height: 100%;
  background-color: #fff;
  display: inline-block;
}
.input-line{
  overflow: hidden;
  padding: 20rpx 50rpx;
}
.input-line label{
  float: left;
  width: 20%;
  line-height: 50rpx;
}
.input-line input{
  width: 70%;
  float: left;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #eee;
  padding: 0 16rpx;
}
.form{
  border-bottom: 1px solid #ddd;
  padding: 20rpx 0;
}

.shopCart-list{
  background-color: #fff;
}
.shopCart-item{
  padding: 30rpx;
  position: relative;
  overflow: hidden;
}
.shopCart-item checkbox{
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
}
.cart-image{
  width: 177rpx;
  height: 132rpx;
  display: block;
  /* margin-left: 70rpx; */
  float: left;
}
.i-info{
  float: left;
  margin-left: 30rpx;
}
.i-name{
  color: #666;
}
.i-price{
  color: #ff6600;
  margin-top: 40rpx;
}
.detele{
  position: absolute;
  width: 44rpx;
  height: 44rpx;
  top: 30rpx;
  right: 30rpx;
}
.i-num{
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
}
.i-minus,.i-add{
  width: 50rpx;
  height: 50rpx;
  float: left;
}
.nums{
  width: 50rpx;
  text-align: center;
  line-height: 50rpx;
  height: 50rpx;
  float: left;
}
.total{
  padding: 30rpx;
  color: #ff6600;
}
.buy-now{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  position: fixed;
  bottom: 0;
  background: #ff6600;
  color: #fff;
  font-size: 32rpx;
}