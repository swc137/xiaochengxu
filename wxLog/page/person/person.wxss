.bg-personal{
  width: 100%;
  height: 326rpx;
  background-color: #fff;
  text-align: center;
  margin-bottom: 20rpx;
}
.icon-head{
  width: 160rpx;
  height: 160rpx;
  margin-top: 60rpx;
  border-radius: 50%;
}
.bg-name{
  font-size: 32rpx;
}
.nav-item{
  height: 110rpx;
  line-height: 110rpx;
  border-bottom: 1px solid #eee;
  padding-left: 30rpx;
  position: relative;
}
.mar-b20{
  margin-bottom: 20rpx;
}
/*一行显示*/
.v-line{
  padding: 20rpx 60rpx 20rpx 30rpx;
  background-color: #fff;
  position: relative;
}
.v-title{
  font-size: 32rpx;
}
.v-icon{
  width: 44rpx;
  height: 44rpx;
  vertical-align: middle;
  margin-right: 6rpx;
}
.right-arrow {
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 30rpx;
  border: none;
  border-left: 0;
  border-bottom: 0;
  border-top: 1px solid #969696;
  border-right: 1px solid #969696;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.fr{
  float: right;
}
.company-about{
  padding: 30rpx;
  background: #fff;
  font-size: 26rpx;
  color: #666;
}
.contact-us{
  padding: 30rpx;
  background: #fff;
  border-bottom: 2rpx solid #ddd;
}